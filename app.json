{
  "expo": {
    "scheme": "Algogene_App",
    "name": "Algogene_App",
    "slug": "Algogene_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.algogene.app",
      "config": {
        "googleSignIn": {
          "reversedClientId": "com.googleusercontent.apps.236411853963-3v3f82rb67e77qqb1hrg6shbt1000v25"
        }
      }
    },
    "android": {
      "package": "com.algogene.app",
      "versionCode": 1, 
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-router", { "root": "./screens" }],
      "expo-dev-client",
      [
        "@react-native-google-signin/google-signin",
        {
          "webClientId": "236411853963-3v3f82rb67e77qqb1hrg6shbt1000v25.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.236411853963-3v3f82rb67e77qqb1hrg6shbt1000v25"
        }
      ]
    ]
  }
}
